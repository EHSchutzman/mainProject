    <children>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#FFFFFF" height="1000.0" layoutX="551.0" layoutY="129.0" stroke="BLACK" strokeType="INSIDE" width="1500.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="100.0" />
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#e8491d" height="110.0" stroke="BLACK" strokeType="INSIDE" width="1500.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#35424a" height="100.0" stroke="BLACK" strokeType="INSIDE" width="1500.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
        <TextField fx:id="edit_username_text" layoutX="289.0" layoutY="171.0" promptText="Username">
            <font>
                <Font size="18.0"/>
            </font>
        </TextField>
        <Group layoutX="576.0" layoutY="182.0">
            <children>
                <TextField fx:id="edit_first_name_text" layoutX="93.0" prefHeight="35.0" prefWidth="193.0"
                           promptText="First Name">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </TextField>
                <TextField fx:id="edit_middle_initial_text" layoutX="95.0" layoutY="47.0" prefHeight="35.0"
                           prefWidth="62.0" promptText="M. I.">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </TextField>
                <TextField fx:id="edit_last_name_text" layoutX="96.0" layoutY="94.0" prefHeight="35.0" prefWidth="186.0"
