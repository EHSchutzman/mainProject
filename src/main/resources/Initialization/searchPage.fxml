<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="500.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.112"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="DatabaseSearch.SearchController">
    <children>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#FFFFFF" height="1000.0" layoutX="551.0" layoutY="129.0" stroke="BLACK" strokeType="INSIDE" width="1500.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="100.0" />
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#e8491d" height="110.0" stroke="BLACK" strokeType="INSIDE" width="1500.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#35424a" height="100.0" stroke="BLACK" strokeType="INSIDE" width="1500.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
        <Label fx:id="colaLabel" layoutX="333.0" layoutY="29.0" prefHeight="53.0" prefWidth="431.0"
               text="COLA Registry Search">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
        <Label layoutX="277.0" layoutY="131.0" text="Date Completed:">
         <font>
            <Font size="18.0" />
         </font></Label>
        <DatePicker fx:id="dpDateRangeStart" layoutX="278.0" layoutY="158.0" promptText="From" />
        <DatePicker fx:id="dpDateRangeEnd" layoutX="505.0" layoutY="158.0" promptText="To" />
        <Label fx:id="errorLabel" layoutX="273.0" layoutY="78.0" prefHeight="17.0" prefWidth="54.0" />
        <TextField fx:id="brand_name_text" layoutX="278.0" layoutY="225.0" promptText="Brand Name"/>
        <TextField fx:id="product_name_text" layoutX="505.0" layoutY="225.0" promptText="Product Name"/>
        <CheckBox fx:id="malt_beverage_checkbox" layoutX="300.0" layoutY="300.0" mnemonicParsing="false"
                  text="Malt Beverage"/>
        <CheckBox fx:id="distilled_spirit_checkbox" layoutX="475.0" layoutY="300.0" mnemonicParsing="false"
                  text="Distilled Spirit"/>
        <CheckBox fx:id="wine_checkbox" layoutX="300.0" layoutY="330.0" mnemonicParsing="false" text="Wine"/>
        <TextField fx:id="state_text" layoutX="278.0" layoutY="393.0" promptText="State"/>
        <Button fx:id="search_button" layoutX="504.0" layoutY="372.0" mnemonicParsing="false"
                onMouseClicked="#handleSearch" prefHeight="31.0" prefWidth="187.0" text="Search">
         <font>
            <Font size="24.0" />
         </font></Button>
        <Button fx:id="return_to_main_button" layoutX="30.0" layoutY="30.0" mnemonicParsing="false"
                onMouseClicked="#returnToMain" text="Return To Main">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Label layoutX="277.0" layoutY="198.0" text="Name Details:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
        <Label layoutX="278.0" layoutY="267.0" text="Product Type:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
        <Label layoutX="278.0" layoutY="366.0" text="Production Location:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
